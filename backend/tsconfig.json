{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "outDir": "./dist",
      "rootDir": "./src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src", "../.vscode/err"],  
    "exclude": ["node_modules"],
    "cSpell.ignorePaths": [
      "**/.vscode/settings.json",
      "**/AppData/Roaming/Code/User/settings.json"
    ]
  }
  
  